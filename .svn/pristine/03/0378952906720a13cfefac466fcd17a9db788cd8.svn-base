#configuring allplication name
spring.application.name=netflix-zuul-api-gateway-server
#configuring port
server.port=8765
#configuring eureka URL
eureka.client.service-url.default-zone=http://localhost:8761/eureka

zuul.routes.authentication-service.path=/zuul-authentiation/**
zuul.routes.authentication-service.url=http://localhost:9001/authenticationServices/public/login

zuul.routes.favourite-service.path=/zuul-favourite/**
zuul.routes.favourite-service.url=http://localhost:9092/favouriteService/private


zuul.sensitiveHeaders= Cookie,Set-Cookie
zuul.host.socket-timeout-millis=300000